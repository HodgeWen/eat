generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model FoodType {
  id   Int    @id @default(autoincrement())
  name String
}

model Food {
  id           Int    @id @default(autoincrement())
  name         String
  foodType String
}
